{
  "redirects": [
    {
      "source": "/.well-known/apple-app-site-association",
      "destination": "/apple-app-site-association.json",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}
